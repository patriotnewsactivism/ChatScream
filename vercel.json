{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/all?path=:path*"
    },
    {
      "source": "/api",
      "destination": "/api/all"
    },
    {
      "source": "/((?!api(?:/|$)|.*\\..*).*)",
      "destination": "/index.html"
    }
  ]
}
